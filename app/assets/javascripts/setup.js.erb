"use strict";

angular.module('elf')
  .config(function($httpProvider, $stateProvider, $urlRouterProvider, $logProvider, config, routes) {
    $httpProvider.defaults.headers.common.Accept = 'application/json';

    $urlRouterProvider.when('', '/dashboard');

    $stateProvider = routes.get($stateProvider);

    $logProvider.debugEnabled(config.is_development);
  }
);

angular.module('elf').run(function(amMoment) {
  amMoment.changeLocale('ru');
});
